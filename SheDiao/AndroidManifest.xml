<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.myrpi.shediao"
    android:versionCode="1"
    android:versionName="1.0" >
	
    <uses-permission android:name="android.permission.INTERNET" > </uses-permission>
    
    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.myrpi.shediao.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.myrpi.shediao.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name="org.myrpi.shediao.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="org.myrpi.shediao.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="org.myrpi.shediao.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
    </application>

</manifest>
